<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<!-- Write a TestScript to execute Multiple Tests in Maven -->
<suite name="Suite">
  <test thread-count="5" name="Test1">
    <classes>
      <class name="test1_Chrome.ChromeDemo"/>
    </classes>
  </test> <!-- Test -->
  
  <test thread-count="5" name="Test2">
    <classes>
      <class name="test1_Edge.EdgeDemo"/>
    </classes>
  </test> <!-- Test -->
  
  <test thread-count="5" name="Test3">
    <classes>
      <class name="test1_FF.FFDemo"/>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
